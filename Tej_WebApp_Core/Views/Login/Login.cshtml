	<link rel="stylesheet" type="text/css" href="login/css/main.css">
	<!--===============================================================================================-->
</head>
<body>

	<div class="limiter">
		<div class="container-login100">
			<div class="wrap-login100">
				<form asp-for="Login" method="post" class="login100-form validate-form p-l-55 p-r-55 p-t-178">
					<span class="login100-form-title" style="background-color:#065492;">
						Sign In
					</span>
					@*					<div class="flex-col-c p-t-10 p-b-40">
					<span class="txt1 p-b-9">

					</span>

					<a href="#" class="txt3">

					</a>
					</div>*@

					<div class="text-center p-t-13 p-b-23">
						<span class="txt1">
							<img src="~/login/images/icons/TejLogoMain.png" class="img-fluid" />
						</span>

						<a href="#" class="txt2">

						</a>
					</div>

					<div class="flex-col-c p-t-10 p-b-40">
						<span class="txt1 p-b-9">

						</span>

						<a href="#" class="txt3">
							@if (ViewBag.ErrorMsg != null)
							{
								<span class="alert alert-danger">
									@ViewBag.ErrorMsg
								</span>
							}
						</a>
					</div>

					<div class="wrap-input100 validate-input m-b-16" data-validate="Please enter username">
						@*<input asp-for="fk_pk_user_key" type="hidden" />*@
						<input asp-for="user_name" class="input100" type="text" name="user_name" placeholder="Username">
						<span class="focus-input100"></span>
					</div>

					<div class="wrap-input100 validate-input" data-validate="Please enter password">
						<input asp-for="password" class="input100" type="password" name="password" placeholder="Password">
						<span class="focus-input100"></span>
					</div>

					@*<div class="text-right p-t-13 p-b-23">
					<span class="txt1">
					Forgot
					</span>

					<a href="#" class="txt2">
					Username / Password?
					</a>
					</div>*@

					<div class="text-right p-t-13 p-b-23">
						<span class="txt1">

						</span>

						<a href="#" class="txt2">

						</a>
					</div>

					<div class="container-login100-form-btn">

						<button class="login100-form-btn" href="@Url.Action("Index", "Home")" style="background-color:#065492;">
							Sign in
						</button>
					</div>



					@*					<div class="flex-col-c p-t-170 p-b-40">
					<span class="txt1 p-b-9">
					Don’t have an account?
					</span>

					<a href="#" class="txt3">
					Sign up now
					</a>
					</div>*@
					<div class="flex-col-c p-t-50 p-b-40">
						<sp an class="txt1 p-b-9">

						</sp>

						<a href="#" class="txt3">

						</a>
					</div>


				</form>
			</div>


		</div>
	</div>


	<!--===============================================================================================-->
	@*<script src="vendor/jquery/jquery-3.2.1.min.js"></script>*@
	<script src="login/vendor/jquery/jquery-3.2.1.min.js"></script>
	<!--===============================================================================================-->
